<phpunit
        bootstrap="../vendor/autoload.php"
        strict="true"
        colors="true">
    <testsuites>
        <testsuite name="dic">
            <directory suffix="Test.php">./unit</directory>
        </testsuite>
    </testsuites>
    <filter>
        <blacklist>
            <directory>./assets</directory>
        </blacklist>
    </filter>
</phpunit>